#!/bin/sh
#SBATCH -J clean_motorbike
#SBATCH -o logs/clean_motorbike.o%J
#SBATCH -e logs/clean_motorbike.e%J
#SBATCH --nodes=2
#SBATCH --ntasks-per-node=36
#SBATCH --ntasks-per-core=1

source /nfs/cluster/intel/oneapi/compiler/latest/env/vars.sh
source /nfs/cluster/intel/oneapi/mpi/latest/env/vars.sh
source /nfs/cluster/app/impi/OpenFOAM-v2206/etc/bashrc

cd ${SLURM_SUBMIT_DIR}

# Source tutorial clean functions
. $WM_PROJECT_DIR/bin/tools/CleanFunctions

# remove surface and features
\rm -rf constant/extendedFeatureEdgeMesh > /dev/null 2>&1
\rm -f constant/triSurface/motorBike.eMesh > /dev/null 2>&1

rm -rf 0 > /dev/null 2>&1
rm -rf ./hostfile

cleanCase
